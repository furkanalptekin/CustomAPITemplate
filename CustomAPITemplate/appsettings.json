{
  "ConnectionStrings": {
    "NpgsqlConnection": "Host=db-server;Database=customapitemplateexample;Username=customapitemplateuser;Password=customapitemplatepasswords"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Elasticsearch" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name":  "Console"
      },
      {
        "Name": "Elasticsearch",
        "Args": {
          "nodeUris": "http://elasticsearch:9200",
          "indexFormat": "apitemplate-logs-{0:yyyy-MM}",
          "autoRegisterTemplate": true,
          "numberOfShards": 2,
          "numberOfReplicas": 1
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName" ],
    "Properties": {
      "Application": "CustomApiTemplate"
    }
  },
  "AllowedHosts": "*",
  "RedisCacheSettings": {
    "IsEnabled": true,
    "ConnectionString": "redis:6379"
  },
  "JwtSettings": {
    "Secret": "BA80686F21D248668DE531E27BEB06E40EC2D4F496194D28841577E8DE277590",
    "TokenLifetime": "00:05:00"
  }
}